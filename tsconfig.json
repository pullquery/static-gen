{
    "include": [
        "./src/**/*.ts",
        "./src/**/*.d.ts"
    ],
    "compilerOptions": {
        "moduleResolution": "Node",
        "baseUrl": "./src",
        "outDir": "./dist",
        "removeComments": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}